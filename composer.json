{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Dynamic Family Tree Builder & Viewer - A modern, interactive family tree application",
    "keywords": ["laravel", "framework", "family-tree", "genealogy", "vue", "inertia"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-activitylog": "^5.0",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-tags": "^4.0",
        "spatie/laravel-translatable": "^6.0",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-json-api-paginate": "^3.0",
        "spatie/laravel-route-attributes": "^3.0",
        "spatie/laravel-settings": "^3.0",
        "spatie/laravel-webhook-client": "^4.0",
        "spatie/laravel-webhook-server": "^4.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^2.0",
        "spatie/laravel-telescope": "^5.0",
        "spatie/laravel-horizon": "^6.0",
        "spatie/laravel-queueable": "^3.0",
        "spatie/laravel-schedule-monitor": "^3.0",
        "spatie/laravel-uptime-monitor": "^3.0",
        "spatie/laravel-http-logger": "^3.0",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-json-api-paginate": "^3.0",
        "spatie/laravel-route-attributes": "^3.0",
        "spatie/laravel-settings": "^3.0",
        "spatie/laravel-webhook-client": "^4.0",
        "spatie/laravel-webhook-server": "^4.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^2.0",
        "spatie/laravel-telescope": "^5.0",
        "spatie/laravel-horizon": "^6.0",
        "spatie/laravel-queueable": "^3.0",
        "spatie/laravel-schedule-monitor": "^3.0",
        "spatie/laravel-uptime-monitor": "^3.0",
        "spatie/laravel-http-logger": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^2.0",
        "spatie/laravel-telescope": "^5.0",
        "spatie/laravel-horizon": "^6.0",
        "spatie/laravel-queueable": "^3.0",
        "spatie/laravel-schedule-monitor": "^3.0",
        "spatie/laravel-uptime-monitor": "^3.0",
        "spatie/laravel-http-logger": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}